{
    "name": "doki1k/parameters",
    "description": "Parameters for Laravel",
    "version": "0.1.0",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/filamentphp/filament/issues",
        "source": "https://github.com/filamentphp/filament"
    },
    "authors": [
        {
            "name": "dOKI1K",
            "email": "lahuen60@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/database": "^10.45|^11.0",
        "illuminate/filesystem": "^10.45|^11.0",
        "illuminate/support": "^10.45|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Doki1k\\Parameters\\": "src/"
        },
        "files": [
            "app/Helpers/helpers-common.php",
            "app/Helpers/helpers-log.php",
            "app/Helpers/helpers-parameters.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Doki1k\\Parameters\\LmpParametersServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}